<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cc.unip.tccfinal.fxml.controller.MenuController">
   <children>
      <VBox>
         <children>
            <VBox>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Label text="Total de Registros a gerar" />
                  <TextField fx:id="tfTotal" />
                  <Button fx:id="btnGerar" maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefWidth="287.0" text="Gerar">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Separator>
                     <VBox.margin>
                        <Insets top="30.0" />
                     </VBox.margin>
                  </Separator>
                  <Button fx:id="btnExcluir" mnemonicParsing="false" prefWidth="286.0" text="Excluir todos registros">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Separator prefWidth="200.0">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Separator>
                  <ProgressIndicator fx:id="progressIndicator" progress="0.0">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </ProgressIndicator>
                  <ProgressBar fx:id="progressBar" prefWidth="283.0" progress="0.0" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</HBox>
